    "client-dev": "npm run dev --prefix ../client",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-build": "tsc",
    "server-dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
